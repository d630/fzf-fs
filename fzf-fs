bind_search+='esc:execute(Fzf::Fs::C::Set normal_mode)',
bind_search+='f1:execute(Fzf::Fs::C::GetKey f1)',


builtin typeset -x bind_set=

bind_set+='btab:toggle-out',
bind_set+='ctrl-i:toggle-in',
bind_set+='ctrl-r:toggle-sort',
